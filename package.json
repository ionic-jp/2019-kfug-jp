      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-ionic-keyboard": {}
    }
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --parser typescript --write",
      "git add"
    ],
    "*.html": [
      "prettier --parser html --write",
      "git add"
    ]
  }
