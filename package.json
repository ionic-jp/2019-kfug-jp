    "@angular-devkit/core": "^8.3.14",
    "@angular-devkit/schematics": "^8.3.14",
    "@angular/cli": "9.0.0-rc.0",
    "@angular/compiler": "9.0.0-rc.1",
    "@angular/compiler-cli": "9.0.0-rc.1",
    "@angular/language-service": "~9.0.0-rc.1",
    "@capacitor/cli": "1.3.0",
