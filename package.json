    "@angular/platform-browser-dynamic": "9.0.0-rc.1",
    "@angular/pwa": "~0.803.17",
    "@angular/router": "9.0.0-rc.1",
    "@angular/service-worker": "~9.0.0-rc.1",
    "@capacitor/core": "1.3.0",
    "@capacitor/ios": "^1.3.0",
